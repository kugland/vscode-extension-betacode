{
  "name": "betacode",
  "displayName": "BetaCode",
  "description": "Type polytonic Greek into VS Code.",
  "version": "0.1.10",
  "publisher": "capatech",
  "license": "GPL-3.0",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/kugland/vscode-extension-betacode"
  },
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "BetaCode",
    "Greek"
  ],
  "activationEvents": [
    "onCommand:extension.betaCode"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.betaCode",
        "title": "Translate BetaCode"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "tslint ./src/*.ts",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "14.17.9",
    "@types/vscode": "1.59.0",
    "tslint": "5.20.1",
    "typescript": "4.3.5"
  }
}
